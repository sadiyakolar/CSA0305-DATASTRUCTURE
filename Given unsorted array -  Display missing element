#include <stdio.h>

int findMissingElement(int arr[], int size, int start, int end) {
    // Calculate expected sum of sequence from start to end
    int expectedSum = (end * (end + 1)) / 2 - ((start - 1) * start) / 2;
    
    // Calculate actual sum of array elements
    int actualSum = 0;
    for (int i = 0; i < size; i++) {
        actualSum += arr[i];
    }
    
    // Missing element is the difference
    return expectedSum - actualSum;
}

int main() {
    int size;
    
    // Input array size
    printf("Enter the size of the array: ");
    scanf("%d", &size);
    
    // Validate array size
    if (size <= 0) {
        printf("Invalid array size\n");
        return 1;
    }
    
    // Declare array and input elements
    int arr[size];
    printf("Enter %d elements (unsorted, one number missing in sequence):\n", size);
    for (int i = 0; i < size; i++) {
        scanf("%d", &arr[i]);
    }
    
    // Assume the sequence starts from 1 and goes up to size + 1
    int start = 1;
    int end = size + 1; // Sequence should have size + 1 elements, one is missing
    
    // Find and display the missing element
    int missing = findMissingElement(arr, size, start, end);
    if (missing >= start && missing <= end) {
        printf("Missing element: %d\n", missing);
    } else {
        printf("No missing element in the expected sequence\n");
    }
    
    return 0;
}
